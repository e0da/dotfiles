{
  "C_Cpp.formatting": "Disabled",
  "breadcrumbs.enabled": true,
  "clang-format.language.javascript.enable": false,
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "explorer.confirmDragAndDrop": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "flow.showUncovered": true,
  "git.autofetch": true,
  "javascript.format.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "keyboard.dispatch": "keyCode",
  "shellformat.flag": "-i 2 -p",
  "shellformat.path": "/home/force/go/bin/shfmt",
  "vim.easymotion": true,
  "vim.easymotionMarkerFontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "vim.easymotionMarkerFontSize": "14",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.easymotionMarkerYOffset": 5,
  "vim.enableNeovim": true,
  "vim.handleKeys": {
    "<C-'>": false,
    "<C-b>": false,
    "<C-d>": true,
    "<C-f>": false,
    "<C-j>": false,
    "<C-k>": false,
    "<C-n>": false,
    "<C-w>": false
  },
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "after": ["<Esc>"],
      "before": ["j", "j"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["<Leader>", "<Leader>", "s"],
      "before": ["<Space>"]
    },
    {
      "after": ["v", "i", "w", "g", "b"],
      "before": ["<C-d>"]
    },
    {
      "after": [],
      "before": ["u"],
      "commands": [
        {
          "args": [],
          "command": "undo"
        }
      ]
    },
    {
      "after": [],
      "before": ["<C-r>"],
      "commands": [
        {
          "args": [],
          "command": "redo"
        }
      ]
    }
  ],
  "vim.textwidth": 80,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "after": ["g", "b"],
      "before": ["<C-d>"]
    }
  ],
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Gruvbox Dark Soft",
  "json.format.enable": false,
  "typescript.format.enable": false,
  "prettier.eslintIntegration": true,
  "cSpell.userWords": [
    "gdpr",
    "gmail",
    "mkdir"
  ],
  "cSpell.language": "en"
}
