[user]
  name = Justin Force
  email = justin.force@gmail.com

[color]
  ui = auto

[alias]
  a  = add
  aa = add --all
  b  = branch
  ci = commit
  co = checkout
  d  = diff
  dc = diff --cached
  pl = pull
  pr = pull --rebase
  ps = push
  s  = status --short
  l  = log --stat
  f  = fetch --all --prune
  mm = merge origin/master
  ff = merge origin/master --ff-only
  m  = merge --no-ff
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  last = cat-file commit HEAD
  fuckit = reset --hard HEAD
  alias = "!zsh -lc \"source ~/.zshrc; git_aliases\""

[rerere]
  enabled = true
[core]
	excludesfile = ~/.gitignore_global
[web]
	browser = google-chrome
[github]
	user = justin.force@gmail.com
